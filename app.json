{
  "expo": {
    "name": "list",
    "slug": "list",
    "description": "um aplicativo para vocÃª organizar suas tarefas!",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/list-icon.png",
    "userInterfaceStyle": "light",
    "scheme": "list",
    "splash": {
      "image": "./src/assets/list-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.your.list"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.your.list"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-font",
        {
          "fonts": [
            {
              "asset": "./src/assets/fonts/Archivo-VariableFont_wdth,wght.ttf",
              "fontWeight": "400",
              "fontStyle": "normal"
            },
            {
              "asset": "./src/assets/fonts/Archivo-VariableFont_wdth,wght.ttf",
              "fontWeight": "700",
              "fontStyle": "normal"
            }
          ]
        }
      ],
      "expo-router"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "59715357-6b41-4d14-98ea-53c41f40a90d"
      }
    },
    "owner": "victorcottar"
  }
}
